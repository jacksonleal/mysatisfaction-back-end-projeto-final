    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sinon": "^8.1.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
